<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.synergyj.bookmule.core.domain.Banco" table="BANCO">
		<id name="id" column="BANCO_ID">
			<generator class="identity" />
		</id>
		<property name="nombre" column="NOMBRE" />
		<property name="urlCargo" column="URL_CARGO" />
	</class>
</hibernate-mapping>
