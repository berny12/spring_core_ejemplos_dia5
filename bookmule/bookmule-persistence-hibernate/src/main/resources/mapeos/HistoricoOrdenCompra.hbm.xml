<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.synergyj.bookmule.core.domain">
    <class name="HistoricoOrdenCompra" table="HISTORICO_STATUS_ORDEN">
        <id name="id" column ="HISTORICO_STATUS_ORDEN_ID">
            <generator class="identity" />
        </id>
        <property name="ordenCompraId" column="ORDEN_COMPRA_ID" />
        <property name="fechaStatus" column="FECHA_STATUS" />
        <property name="statusOrdenCompra" type="statusOrdenCompra"/>
    </class>
</hibernate-mapping>
