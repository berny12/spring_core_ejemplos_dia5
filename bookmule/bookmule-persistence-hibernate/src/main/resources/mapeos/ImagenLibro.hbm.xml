<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.synergyj.bookmule.core.domain.ImagenLibro" table="IMAGEN_LIBRO">
        <id name="id" column ="IMAGEN_LIBRO_ID">
            <generator class="identity" />
        </id>
        <property name="numImagen" column="NUM_IMAGEN" />
    	<property name="imagen" column="IMAGEN"/>
    </class>
</hibernate-mapping>
